{
  "name": "interaction-viewer",
  "version": "1.0.1",
  "description": "Display interaction information for different proteins and overlay information about diseases and subcellular location.",
  "main": "src/main.js",
  "scripts": {
    "rollup": "rollup -cw",
    "test": "mocha",
    "sass:dist": "node-sass styles/main.scss > dist/style.min.css",
    "sass": "npm run sass:dist",
    "serve": "browser-sync start --server -f dist/**/*.*",
    "deploy": "git push origin `git subtree split --prefix dist master`:gh-pages --force",
    "test-watch": "mocha -w"
  },
  "keywords": [
    "interaction",
    "visualization",
    "biojs",
    "uniprot",
    "ebi"
  ],
  "author": "Xavier Watkins <xwatkins@ebi.ac.uk>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "browser-sync": "^2.12.5",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "cssnano": "^3.10.0",
    "mocha": "^3.3.0",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "uglify-js": "^2.7.4"
  },
  "dependencies": {
    "d3": "^4.10.2",
    "underscore-es": "^1.9.8",
    "webcomponentsjs": "^1.0.2"
  }
}
